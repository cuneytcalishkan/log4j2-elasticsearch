{
  "index_patterns": [
    "${sys:indexTemplate.indexPatterns}"
  ],
  "settings": {
    "index": {
      "refresh_interval": "${sys:indexTemplate.settings.index.refreshInterval}"
    }
  },
  "mappings": {
    "properties": {
      "loggerName": {
        "type": "keyword",
        "index": false
      },
      "message": {
        "type": "keyword",
        "index": false
      },
      "timeMillis": {
        "type": "date",
        "format": "epoch_millis"
      },
      "thread": {
        "type": "keyword",
        "index": false
      },
      "level": {
        "type": "keyword",
        "index": false
      }
    }
  }
}
